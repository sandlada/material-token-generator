{
    "name": "@sandlada/material-token-generator",
    "version": "1.0.0.nightly-20250809a",
    "author": "Kai-Orion & Sandlada",
    "license": "MIT",
    "type": "module",
    "description": "Use the @material/material-color-utilities tool to create a set of tokens based on material design.",
    "keywords": [
        "color",
        "theme-generator",
        "material",
        "material-design",
        "material-tokens",
        "tokens",
        "css",
        "color-tokens"
    ],
    "publishConfig": {
        "access": "public"
    },
    "main": "build/index.js",
    "module": "build/index.js",
    "files": [
        "build/**/*",
        "README.md"
    ],
    "exports": {
        ".": {
            "import": "./build/index.js",
            "require": "./build/index.js",
            "types": "./build/index.d.ts",
            "default": "./build/index.js"
        },
        "./*": {
            "import": "./*.js",
            "require": "./*.js",
            "types": "./build/*.d.ts",
            "default": "./*.js"
        }
    },
    "scripts": {
        "compile": "rollup --config"
    },
    "devDependencies": {
        "@material/material-color-utilities": "^0.3.0",
        "@rollup/plugin-node-resolve": "^16.0.1",
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.3",
        "@types/node": "24.0.4",
        "rollup": "^4.44.1",
        "tslib": "^2.8.1",
        "typescript": "~5.8.3"
    },
    "workspaces": [
        "dev-app"
    ]
}
